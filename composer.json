{
    "name": "laravoltech/laravoltech",
    "description": "Laravoltech is a PHP HTTP client library",
    "keywords": [
        "laravoltech",
        "laravoltech"
    ],
    "homepage": "https://github.com/laravoltech/laravoltech",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kalvin Founder",
            "email": "laravoltech@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laravoltech\\Laravoltech\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravoltech\\Laravoltech\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravoltech\\Laravoltech\\LaravoltechServiceProvider"
            ],
            "aliases": {
                "Laravoltech": "Laravoltech\\Laravoltech\\LaravoltechFacade"
            }
        }
    }
}
